{% extends "::base.html.twig" %}

{% block body %}
    <div>
{% for zone in zones %}
    
    {{ zone.id }}
    {{ zone.nom }}
    {{ zone.couleur }}
    {{ zone.heureDate|date() }}
    {{ zone.action }}
    {{ zone.utilisateur }}
    {% for point in zone.points %}
       x={{ point.x }}, y={{point.y }}, position={{ point.position }} 
    {% endfor %}
{% endfor %}

</div>
{% endblock %}